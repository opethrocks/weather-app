AAA,MAAM,CAAC,cAAD,EAAiBY,QAAQ,CAACpB,IAA1B,CAAN;AACA,OAPF;AAQA,KAlDO;AAmDR6B,IAAAA,UAnDQ,6BAmDe;AAAA,UAAVrB,MAAU,SAAVA,MAAU;AACtBA,MAAAA,MAAM,CAAC,aAAD,CAAN;AACA,KArDO;AAsDRsB,IAAAA,cAtDQ,iCAsDmB;AAAA,UAAVtB,MAAU,SAAVA,MAAU;AAC1BA,MAAAA,MAAM,CAAC,iBAAD,CAAN;AACA;AAxDO,GAnCoB;AA8F7BuB,EAAAA,OAAO,EAAE;AACRvC,IAAAA,cAAc,EAAE,wBAACD,KAAD;AAAA,aAAWA,KAAK,CAACC,cAAjB;AAAA,KADR;AAERC,IAAAA,eAAe,EAAE,yBAACF,KAAD;AAAA,aAAWA,KAAK,CAACE,eAAjB;AAAA,KAFT;AAGRuC,IAAAA,YAAY,EAAE,sBAACzC,KAAD;AAAA,aAAWA,KAAK,CAACG,aAAjB;AAAA,KAHN;AAIRoC,IAAAA,cAAc,EAAE,wBAACvC,KAAD;AAAA,aAAWA,KAAK,CAACI,gBAAjB;AAAA,KAJR;AAKRC,IAAAA,OAAO,EAAE,iBAACL,KAAD;AAAA,aAAWA,KAAK,CAACK,OAAjB;AAAA;AALD;AA9FoB,CAAf,CAAf","sourcesContent":["import Vue from \"vue\";\nimport Vuex from \"vuex\";\nimport axios from \"axios\";\n\nVue.use(Vuex);\n\nexport default new Vuex.Store({\n\tstate: {\n\t\tcurrentWeather: null,\n\t\tweatherForecast: null,\n\t\tselectedUnits: \"\",\n\t\tisForecastActive: false,\n\t\tmapData: null,\n\t},\n\tmutations: {\n\t\tADD_WEATHER(state, payload) {\n\t\t\tlet data = payload;\n\t\t\tstate.currentWeather = data;\n\t\t},\n\t\tADD_FORECAST(state, payload) {\n\t\t\tlet data = payload;\n\t\t\tstate.weatherForecast = data;\n\t\t},\n\t\tADD_MAP_DATA(state, payload) {\n\t\t\tlet data = payload;\n\t\t\tstate.mapData = data;\n\t\t},\n\t\tSET_UNIT(state, payload) {\n\t\t\tlet unit = payload;\n\t\t\tstate.selectedUnits = unit;\n\t\t},\n\t\tDELETE_CITY(state) {\n\t\t\tstate.currentWeather = null;\n\t\t\tstate.weatherForecast = null;\n\t\t},\n\t\tTOGGLE_FORECAST(state) {\n\t\t\tstate.isForecastActive === false\n\t\t\t\t? (state.isForecastActive = true)\n\t\t\t\t: (state.isForecastActive = false);\n\t\t},\n\t},\n\tactions: {\n\t\tcurrentWeather({ commit }, payload) {\n\t\t\tlet city = encodeURIComponent(payload.city);\n\t\t\tlet unit = payload.unit;\n\t\t\tconst apiKey = process.env.VUE_APP_API_KEY;\n\t\t\taxios({\n\t\t\t\tmethod: \"GET\",\n\t\t\t\turl: `https://api.openweathermap.org/data/2.5/weather?q=${city}&appid=${apiKey}`,\n\t\t\t\tparams: {\n\t\t\t\t\tunits: unit,\n\t\t\t\t},\n\t\t\t})\n\t\t\t\t.then((response) => {\n\t\t\t\t\tcommit(\"ADD_WEATHER\", response.data);\n\t\t\t\t\tcommit(\"SET_UNIT\", unit);\n\t\t\t\t})\n\t\t\t\t.catch((error) => {\n\t\t\t\t\talert(error);\n\t\t\t\t});\n\t\t},\n\t\tweatherForecast({ commit }, payload) {\n\t\t\tlet city = encodeURIComponent(payload.city);\n\t\t\tlet unit = payload.unit;\n\t\t\tconst apiKey = process.env.VUE_APP_API_KEY;\n\t\t\taxios({\n\t\t\t\tmethod: \"GET\",\n\t\t\t\turl: `https://api.openweathermap.org/data/2.5/forecast?q=${city}&appid=${apiKey}`,\n\t\t\t\tparams: {\n\t\t\t\t\tunits: unit,\n\t\t\t\t},\n\t\t\t})\n\t\t\t\t.then((response) => {\n\t\t\t\t\tcommit(\"ADD_FORECAST\", response.data);\n\t\t\t\t\tcommit(\"SET_UNIT\", unit);\n\t\t\t\t})\n\t\t\t\t.catch((error) => {\n\t\t\t\t\talert(error);\n\t\t\t\t});\n\t\t},\n\t\tgetMapData({ commit }) {\n\t\t\tconst apiKey = process.env.VUE_APP_API_KEY;\n\t\t\tconst date = new Date().toUTCString();\n\t\t\taxios\n\t\t\t\t.get(\n\t\t\t\t\t`http://maps.openweathermap.org/maps/2.0/weather/TA2/{z}/{x}/{y}?date=${date}&\nopacity=0.9&fill_bound=true&appid=${apiKey} `\n\t\t\t\t)\n\t\t\t\t.then((response) => {\n\t\t\t\t\tcommit(\"ADD_MAP_DATA\", response.data);\n\t\t\t\t});\n\t\t},\n\t\tdeleteCity({ commit }) {\n\t\t\tcommit(\"DELETE_CITY\");\n\t\t},\n\t\ttoggleForecast({ commit }) {\n\t\t\tcommit(\"TOGGLE_FORECAST\");\n\t\t},\n\t},\n\n\tgetters: {\n\t\tcurrentWeather: (state) => state.currentWeather,\n\t\tweatherForecast: (state) => state.weatherForecast,\n\t\tselectedUnit: (state) => state.selectedUnits,\n\t\ttoggleForecast: (state) => state.isForecastActive,\n\t\tmapData: (state) => state.mapData,\n\t},\n});\n"]}]}